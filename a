#include <stdio.h>
#include <conio.h>
#include <string.h>
int k[50];
int i,n;
void main()
{
clrscr(); printf("dizinin eleman sayisi: "); scanf("%d",&n);
for (i=0;i<n;i++) {printf("%d. eleman: ",i+1); scanf("%d",&k[i]);}
printf("Cift= ");
for (i=0;i<n;i++)
{
if (k[i]%2==0) printf("%d\t",k[i]);
else continue;
}
printf("\nTek= ");
for (i=0;i<n;i++)
{
if (k[i]%2!=0) printf("%d\t",k[i]);
else continue;
}
getch();
